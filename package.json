{
  "name": "jquery-js-refactor",
  "version": "0.0.0",
  "private": true,
  "description": "Modern jQuery: Refactoring and Testing the Way Forward",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "keywords": [
    "jquery",
    "refactor",
    "test"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kendaleiv/jquery-js-refactor"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "webpack -d",
    "check": "npm run audit && npm outdated --depth 0",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html npm test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "eslint src test",
    "start": "npm run build && node server.js",
    "start:watch": "concurrently -k -r \"npm run build -- --watch\" \"node server.js\"",
    "test": "ava",
    "validate": "npm run lint && npm test"
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "author": {
    "name": "Ken Dale",
    "email": "ken@kendaleiv.com",
    "url": "http://kendaleiv.com"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "express": "^4.14.0",
    "jquery": "^1.10.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.5.0",
    "concurrently": "^2.2.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.3.1",
    "fetch-mock": "^5.1.3",
    "jsdom": "^9.5.0",
    "nsp": "^2.2.0",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.2",
    "sinon": "^1.17.5",
    "webpack": "^1.13.2"
  }
}
