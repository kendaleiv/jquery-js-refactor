{
  "name": "jquery-js-refactor",
  "version": "0.0.0",
  "private": true,
  "description": "Modern jQuery: Refactoring and Testing the Way Forward",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "keywords": [
    "jquery",
    "refactor",
    "test"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kendaleiv/jquery-js-refactor"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "webpack --mode production",
    "check": "npm run audit && npm outdated --depth 0",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html npm test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "eslint src test",
    "start": "npm run build && node server.js",
    "start:watch": "concurrently -k -r \"npm run build -- --watch\" \"node server.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "npm run lint && npm test"
  },
  "author": {
    "name": "Ken Dale",
    "email": "ken@kendaleiv.com",
    "url": "https://kendaleiv.com"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "express": "^4.16.2",
    "jquery": "^1.10.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "jest": "^22.4.2",
    "jest-fetch-mock": "^1.4.2",
    "nsp": "^3.2.1",
    "pre-commit": "^1.2.2",
    "sinon": "^4.4.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  }
}
